{
  "name": "@adobe/helix-dispatch",
  "version": "2.1.4",
  "description": "Example Package",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "test": "nyc --reporter=text --reporter=lcov --check-coverage --branches 97 --statements 100 --lines 100 mocha -i -g 'Post-Deploy'",
    "test-ci": "nyc --reporter=text --reporter=lcov --check-coverage --branches 97 --statements 100 --lines 100 mocha --reporter xunit --reporter-options output=./junit/test-results.xml -i -g 'Post-Deploy' && codecov",
    "test-postdeploy": "mocha --reporter xunit --reporter-options output=./junit/test-results.xml -g 'Post-Deploy'",
    "lint": "./node_modules/.bin/eslint .",
    "semantic-release": "semantic-release",
    "docs": "npx jsdoc2md -c .jsdoc.json --files 'src/*.js'  > docs/API.md",
    "build": "wsk-builder -v",
    "deploy": "wsk-builder -v --deploy --test",
    "deploy-sequences": "wsk-builder --no-build -no-hints -l latest -l major -l minor",
    "deploy-ci": "wsk-builder -v --deploy --test=/ping --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci"
  },
  "wsk": {
    "name": "helix-services/dispatch@${version}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-dispatch"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-dispatch/issues"
  },
  "homepage": "https://github.com/adobe/helix-dispatch#readme",
  "dependencies": {
    "@adobe/helix-status": "^5.0.1",
    "@adobe/openwhisk-action-utils": "^2.3.2",
    "epsagon": "^1.37.0",
    "openwhisk": "^3.20.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "^1.1.0",
    "@adobe/openwhisk-action-builder": "^2.1.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/exec": "^3.3.7",
    "@semantic-release/git": "^7.0.14",
    "@semantic-release/github": "^5.4.3",
    "ajv": "^6.10.2",
    "bunyan": "^1.8.12",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codecov": "^3.6.1",
    "eslint": "^6.5.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc-to-markdown": "^5.0.1",
    "junit-report-builder": "^1.3.2",
    "lint-staged": "^9.4.0",
    "lodash": "^4.17.15",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.1",
    "semantic-release": "^15.13.24"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "statuspage": {
    "name": "Helix Dispatch",
    "group": "Delivery"
  },
  "newrelic": {
    "group_policy": "Delivery Repeated Failure"
  }
}
