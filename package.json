{
  "name": "@adobe/helix-experimental-dispatch",
  "version": "1.3.0",
  "description": "Example Package",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc --reporter=text --reporter=lcov --check-coverage --branches 100 --statements 100 --lines 100 mocha -i -g 'Post-Deploy'",
    "test-ci": "nyc --reporter=text --reporter=lcov --check-coverage --branches 100 --statements 100 --lines 100 mocha --reporter xunit --reporter-options output=./junit/test-results.xml -i -g 'Post-Deploy' && codecov",
    "test-postdeploy": "mocha --reporter xunit --reporter-options output=./junit/test-results.xml -g 'Post-Deploy'",
    "lint": "./node_modules/.bin/eslint .",
    "semantic-release": "semantic-release",
    "semantic-prepare": "node ./build/prepare.js",
    "docs": "npx jsdoc2md -c .jsdoc.json --files 'src/*.js'  > docs/API.md",
    "commit": "git-cz",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "build": "wsk-builder -v",
    "deploy": "wsk-builder -v --deploy --test -p EPSAGON_TOKEN $EPSAGON_TOKEN"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-experimental-dispatch"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-experimental-dispatch/issues"
  },
  "homepage": "https://github.com/adobe/helix-experimental-dispatch#readme",
  "dependencies": {
    "epsagon": "^1.27.1"
  },
  "devDependencies": {
    "@adobe/openwhisk-action-builder": "^1.0.2",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/exec": "^3.3.5",
    "@semantic-release/git": "^7.0.14",
    "@semantic-release/github": "^5.4.2",
    "@semantic-release/npm": "^5.1.13",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "ajv": "^6.10.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codecov": "^3.3.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc-to-markdown": "^5.0.0",
    "junit-report-builder": "^1.3.2",
    "lint-staged": "^8.1.5",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0",
    "proxyquire": "^2.1.0",
    "semantic-release": "^15.13.18",
    "snyk": "^1.185.1"
  },
  "snyk": true,
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "wsk": {
    "name": "helix-services/experimental-dispatch@1.3.0",
    "static": []
  }
}
